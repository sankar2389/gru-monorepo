  "workspaces": {
    "packages": [
      "packages/web",
      "packages/components"
    ]
  },
  "scripts": {
    "clean": "yarn workspaces run clean",
    "dev:web": "yarn workspace @gru/web compile && concurrently --kill-others \"yarn workspace @gru/web compile -w\" \"yarn workspace @gru/web start\"",
    "dev:cms": "cd packages/cms && npm start"
  },
